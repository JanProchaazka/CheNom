pocet		:== desitky					+{INT::NEW($1.int);}										//    1 -   99
			  | desitky+stovky			+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}					//  101 -  999
			  | desitky+stovky+tisice	+{INT::ADD($0,$1);INT::ADD($0,$2);INT::ADD($0,$3);}			// 1101 - 9999
			  | stovky					+{INT::NEW($1.int);}										//  1.. -  9..
			  | desitky+tisice			+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}					// 1.01 - 9.99
			  | stovky+tisice			+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}					// 11.. - 99..
			  | tisice					+{INT::NEW($1.int);}										// 1... - 9...
			  ;
