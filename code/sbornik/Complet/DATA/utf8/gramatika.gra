				  ;

halogenid	:== halogen		+{MOL::NEW([(~.~,#1,{})]:[0-1]);MOL::APL($1,$0,#1);MOL::CPY($1);}
			  ;

halogen	:== 'chlor'		+{MOL::NWA(~Cl~);}	//Cl
		  | 'fluor'		+{MOL::NWA(~F~);}	//F
		  | 'brom'		+{MOL::NWA(~Br~);}	//Br
		  | 'jod'		+{MOL::NWA(~I~);}	//I
		  | 'astat'		+{MOL::NWA(~At~);}	//At
		  ;
