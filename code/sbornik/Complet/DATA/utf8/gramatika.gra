		  | 'sila'			+{MOL::NWA(~Si~);}
		  ;
--============================================================================--
--                             FUNKCNI PRINCIP                                --
--============================================================================--
skelet_funkcne	:== 'di'+pref_subst+fc_skupina_2				+{MOL::CPY($3);MOL::CON($0,$2,#2);}
				  | pref_subst+pref_subst+fc_skupina_2			+{MOL::CPY($3);MOL::CON($0,$1,#1);MOL::CON($0,$2,#1);}
				  | pref_subst+fc_skupina_1						+{MOL::CPY($2);MOL::CON($0,$1,#1);}
				  ;
fc_skupina_1	:== 'aldehyd'			+{MOL::NEW([(~C~,#4,{~1~})-(~O~,#2,{})]:[1=2]);}
				  | halogen+'id'		+{MOL::CPY($1);}	//chlorid, fluorid, ..
				  ;
fc_skupina_2	:== 'keton'			+{MOL::NEW([(~C~,#4,{~1~})-(~O~,#2,{})]:[1=2]);}
				  | 'ether'			+{MOL::NWA(~O~);}
				  | 'oxid'+'an'		+{MOL::NWA(~O~);}
				  | 'sulf'+'an'		+{MOL::NWA(~S~);}
				  | 'sulf'+'id'		+{MOL::NWA(~S~);}
--				  | 'per'+'oxid'	+{MOL::NEW([(~O~,#2,{~1~})-(~O~,#2,{~2~})]:[1-2]);}
				  ;

