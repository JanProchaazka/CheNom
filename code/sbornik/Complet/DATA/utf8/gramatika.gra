-- LOKANTY
-- R-3.1.2-3 (sudy pocet lokantu)
lokanty2a	:== lokant2							+{MOL::LAP($0,$1);}
			  | lokant2+','+lokanty2a			+{MOL::LAP($0,$1);MOL::LAP($0,$3);}
			  ;
------------
-- R-0.1.3.3 (seznam dvojic lokantu)
lokanty2	:== lokant2							+{MOL::LAP($0,$1);}
			  | lokant2+':'+lokanty2			+{MOL::LAP($0,$1);MOL::LAP($0,$3);}
			  ;
------------
			-- dva lokanty
lokant2		:== lokant_+','+lokant_				+{MOL::ZDO($0,$1,$3);}
			  ;
			-- lib.
------------
-- R-0.1.3.1 (lib. pocet lokantu)
lokanty		:== lokant+','+lokant		+{MOL::LAP($0,$1);MOL::LAP($0,$3);}
			  | lokant+','+lokanty		+{MOL::LAP($0,$1);MOL::LAP($0,$3);}
			  ;
------------
			-- (jeden lokant)
lokant		:== lokant_					+{MOL::ZDO($0,$1);}
			  | lokant_+'('+lokant_+')'	+{MOL::ZDO($0,$1,$3);} //2(6)-en		R.0.1.5.1(d)
			  --| '('+'<i>'+stereo+'</i>'+')'		+{}
			  | recke					+{STR::NEW($1.str);MOL::ZDO($0,$0);}
			  --| prvek					+{STR::NEW($1.str);}
			  ;
			-- ("ciselny lokant")
lokant_		:== cislo					+{STR::NEW($1.str);}
			  | cislo+carky				+{STR::NEW($1.str);STR::APP($0.str,$2.str);}
			  --| cislo+latinske			+{STR::APP($1.str,$2.str);}
			  ;

cislo		:== cislice					+{STR::NEW($1.str);}
			  | cislice+cislo			+{STR::APP($1.str,$2.str);STR::NEW($1.str);}
			  ;
cislice		:== '0'						+{STR::NEW(~0~);}
			  | '1'						+{STR::NEW(~1~);}
			  | '2'						+{STR::NEW(~2~);}
			  | '3'						+{STR::NEW(~3~);}
			  | '4'						+{STR::NEW(~4~);}
			  | '5'						+{STR::NEW(~5~);}
			  | '6'						+{STR::NEW(~6~);}
			  | '7'						+{STR::NEW(~7~);}
			  | '8'						+{STR::NEW(~8~);}
			  | '9'						+{STR::NEW(~9~);}
			  ;
recke	:== '&alpha;'		+{STR::NEW(~&alpha;~);}
		  | '&beta;'		+{STR::NEW(~&beta;~);}
		  | '&gamma;'		+{STR::NEW(~&gamma;~);}
		  | '&delta;'		+{STR::NEW(~&delta;~);}
		  | '&epsilon;'		+{STR::NEW(~&epsilon;~);}
		  ;
-- R-1.2.4.2, R-1.2.8.2
carky		:== '''						+{STR::NEW(~'~);}
			  | '''+carky				+{STR::NEW($2);STR::APP($0,~'~);}
			  ;
