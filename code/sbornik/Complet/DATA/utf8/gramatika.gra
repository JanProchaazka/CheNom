			  | 'tri'					+{INT::NEW(#3);}		// 3
			  | pravjednotky			+{INT::NEW($1.int);}	// [4-9]
			  ;
pravjednotky:== 'tetra'					+{INT::NEW(#4);}		// 4
			  | 'penta'					+{INT::NEW(#5);}		// 5
