			  ;
desitky		:== 'mono'					+{INT::NEW(#1);}		// ^1$
			  | 'di'					+{INT::NEW(#2);}		// ^2$
			  | 'tri'					+{INT::NEW(#3);}		// ^3$
			  | desitky_nas				+{INT::NEW($1.int);}	// ^[4-9]$
			  ;
stovky		:== 'hekta'					+{INT::NEW(#100);}		// 100
			  | 'dikta'					+{INT::NEW(#200);}		// 200
			  | 'trikta'				+{INT::NEW(#300);}		// 300
			  | pravjednotky+'kta'		+{INT::NEW($1);INT::MUL($0,#100);}	// [4-9]00
			  ;
tisice		:== 'kilia'					+{INT::NEW(#1000);}	// 1000
			  | 'dilia'					+{INT::NEW(#2000);}	// 2000
			  | 'trilia'				+{INT::NEW(#3000);}	// 3000
			  | pravjednotky+'lia'		+{INT::NEW($1);INT::MUL($0,#1000);}	// [4-9]000
			  ;
------------
-- R-0.1.4.2
pocet_nas	:== 'bis'					+{INT::NEW(#2);}						//    2 - 2
			  | 'tris'					+{INT::NEW(#3);}						//    3 - 3
			  | pocet_nas_p+'kis'		+{INT::NEW($1.int);}					//    4 - 9999
			  ;
pocet_nas_p	:== desitky_nas					+{INT::NEW($1.int);}								//    4 - 99
			  | desitky_nas+stovky			+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}			//  104 - 999
			  | desitky_nas+stovky+tisice	+{INT::ADD($0,$1);INT::ADD($0,$2);INT::ADD($0,$3);}	// 1101 - 9999
			  | stovky						+{INT::NEW($1.int);}								//  1.. - 9..
			  | desitky_nas+tisice			+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}			// 1.01 - 9.99
			  | stovky+tisice				+{INT::ADD($0.int,$1.int);INT::ADD($0,$2);}			// 11.. - 99..
			  | tisice						+{INT::NEW($1.int);}								// 1... - 9...
			  ;
desitky_nas	:== pravjednotky			+{INT::NEW($1);}	// ^[4-9]$
			  | 'deka'					+{INT::NEW(#10);}	// 10
			  | 'undeka'				+{INT::NEW(#11);}	// 11
			  | 'dodeka'				+{INT::NEW(#12);}	// 12
			  | 'trideka'				+{INT::NEW(#13);}	// 13
			  | pravjednotky+'deka'		+{INT::NEW(#10);INT::ADD($0,$1);}	// 1[4-9]
			  | 'ikosa'					+{INT::NEW(#20);}	// 20
			  | jednotky+'kosa'			+{INT::NEW(#20);INT::ADD($0,$1);}	// 2[1-9]
			  | 'triakonta'				+{INT::NEW(#30);}	// 30
			  | jednotky+'triakonta'	+{INT::NEW(#30);INT::ADD($0,$1);}	// 3[1-9]
			  | pravjednotky+'konta'				+{INT::NEW($1);INT::MUL($0,#10);}				// [4-9]0
			  | jednotky+pravjednotky+'konta'		+{INT::ADD($0,$1);INT::MUL($2,#10);ADD($0,$2);}	// [4-9][1-9]
			  ;
------------
-- R-0.1.4.3
pref_nasob	:== 'bi'							+{INT::NEW(#2);}	// 2
			  | 'ter'							+{INT::NEW(#3);}	// 3
			  | 'kvater'						+{INT::NEW(#4);}	// 4
			  | 'kvinkre'						+{INT::NEW(#5);}	// 5
			  | 'sexi'							+{INT::NEW(#6);}	// 6
			  | 'septi'							+{INT::NEW(#7);}	// 7
			  | 'okti'							+{INT::NEW(#8);}	// 8
			  | 'novi'							+{INT::NEW(#9);}	// 9
			  | 'deci'							+{INT::NEW(#10);}	//10
			  | 'undeci'						+{INT::NEW(#11);}	//11
			  | 'dodeci'						+{INT::NEW(#12);}	//12
			  | 'trideci'						+{INT::NEW(#13);}	//13
			  --| ... // ???
			  ;



