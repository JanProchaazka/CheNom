################################### pohledy ###################################
Viewpoint {
	position 0 0 50
	description "above"
}
Viewpoint {
	position -50 0 0
	orientation 0 1 0 -1.571
	description "left"
}
Viewpoint {
	position 0 -50 0
	orientation 1 0 0 1.571
	description "front"
