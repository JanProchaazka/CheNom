################################# atom - vzor #################################
###############################################################################
PROTO Atom[
		field SFColor dCol 1.0 1.0 1.0
		field SFFloat fRad 1.
	  ]{
	Transform{
		children[
			Shape{
				geometry Sphere {
					radius .5					#polomer vnitrni casti
				}
				appearance
					Appearance{
						material Material {
							diffuseColor IS dCol
							transparency 0.		#vnitrni cast - viditelnost
						}
					}
			}
			Shape{
				geometry Sphere {
					radius IS fRad
				}
				appearance
					Appearance{
						material Material {
							diffuseColor IS dCol
							transparency .5		#vnejsi cast - viditelnost
						}
					}
			}
		]
	}
}

################################ vazby - vzory ################################
###############################################################################
PROTO Spoj[
		field SFColor dCol 1.0 1.0 1.0
		field SFFloat fLen 2.
		field SFVec3f fPos 0 1 0.	#0 fLen/2 0
		field SFFloat fRad .5
	  ]{
	Transform{
		translation IS fPos
		children[
			Shape{
				geometry Cylinder {
					height IS fLen
					radius IS fRad
				}
				appearance
				Appearance{
					material Material {
						diffuseColor IS dCol
						transparency 0
					}
				}
			}
		]
	}
}

PROTO Vazba_err [
		field SFFloat fDist 2.
		field SFVec3f fPoz  0 1 0.
	  ]{
	Spoj {
		dCol 1.0 0.0 0.0
		fLen IS fDist
		fPos IS fPoz
		fRad .25
	}
}

